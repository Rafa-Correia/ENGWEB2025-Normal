extends layout

block content
    .w3-card-4
        header.w3-container.w3-blue
            h1= tit
        w3.container 
            table.w3-table-all 
                tr 
                    th Id 
                    th Ano de Edição
                    th Organizador
                    th Vencedor
                each ed in editionList
                    tr 
                        td
                            a(href="/" + ed._id)= ed._id
                        td= ed.anoEdição
                        td
                            a(href="paises/" + ed.organizacao)= ed.organizacao
                        td
                            a(href="paises/" + ed.vencedor)= ed.vencedor
    footer.w3-container.w3-blue 
        address Generated by ENGWEB2025